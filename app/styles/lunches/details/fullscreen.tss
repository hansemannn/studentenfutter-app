
"#window": {
	backgroundColor: "black",
	barColor: "#111"
}

"#window[platform=ios]": {
	translucent: true,
	largeTitleEnabled: false
}

"#image": {
	width: Ti.Platform.displayCaps.platformWidth
}

".scrollView": {
	contentWidth: 'auto',
	contentHeight: 'auto',
	top: 0,
	bottom: 0,
	backgroundColor: '#000',
	showVerticalScrollIndicator: false,
	showHorizontalScrollIndicator: false,
	maxZoomScale: 2,
	minZoomScale: 1,
	zoomScale: 1,
}
