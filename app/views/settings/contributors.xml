<Alloy>
	<Window id="window" titleid="contributors" onOpen="populateContributors">
	<ListView id="list" defaultItemTemplate="contrib">
		<Templates>
			<ItemTemplate name="contrib" class="cell">
				<ImageView class="avatar" bindId="avatar" />
				<Label class="title" bindId="title" />
				<Label class="subtitle" bindId="subtitle" />
			</ItemTemplate>
		</Templates>
		<ListSection id="section" />
	</ListView>
</Window>
</Alloy>
